{
  "version" : 1,
  "index_patterns" : [
    {
      "es": {
        "default_index": "logstash-*"
      },
      "tail_interval_in_seconds": 10,
      "es_index_time_offset_in_seconds": 0,
      "display_timezone": "local",
      "display_timestamp_format": "MMM DD HH:mm:ss",
      "max_buckets": 500,
      "default_time_range_in_days" : 0,
      "max_hosts": 100,
      "max_events_to_keep_in_viewer": 5000,
      "keyword_suffix": "",
      "fields" : {
        "mapping" : {
          "timestamp" : "logtimestamp",
          "hostname" : "beat.name",
          "program": "fields.log_type",
          "message": "message"
        },
        "message_format": "{{{logmessage}}}"
      },
      "color_mapping" : {
      }
    },
    {
      "es": {
        "default_index": "filebeat-*-nginx-*"
      },
      "tail_interval_in_seconds": 10,
      "es_index_time_offset_in_seconds": 0,
      "display_timezone": "local",
      "display_timestamp_format": "MMM DD HH:mm:ss",
      "max_buckets": 500,
      "default_time_range_in_days" : 0,
      "max_hosts": 100,
      "max_events_to_keep_in_viewer": 5000,
      "keyword_suffix": "",
      "fields" : {
        "mapping" : {
          "timestamp" : "@timestamp",
          "hostname" : "host.name",
          "program": "fileset.module",
          "message": "source"
        },
        "message_format":"{{{fileset.name}}} | {{{nginx.access.url}}} | {{{nginx.access.body_sent.bytes}}} | {{{nginx.error.message}}}"
      },
      "color_mapping" : {
      }
    },
    {
      "es": {
        "default_index": "filebeat-*-system-*"
      },
      "tail_interval_in_seconds": 10,
      "es_index_time_offset_in_seconds": 0,
      "display_timezone": "local",
      "display_timestamp_format": "MMM DD HH:mm:ss",
      "max_buckets": 500,
      "default_time_range_in_days" : 0,
      "max_hosts": 100,
      "max_events_to_keep_in_viewer": 5000,
      "keyword_suffix": "",
      "fields" : {
        "mapping" : {
          "timestamp" : "@timestamp",
          "hostname" : "host.name",
          "program": "fileset.name",
          "message": "source"
        },
        "message_format": "{{{system.syslog.message}}}"
      },
      "color_mapping" : {
      }
    },
    {
      "es": {
        "default_index": "filebeat-*"
      },
      "tail_interval_in_seconds": 10,
      "es_index_time_offset_in_seconds": 0,
      "display_timezone": "local",
      "display_timestamp_format": "MMM DD HH:mm:ss",
      "max_buckets": 500,
      "default_time_range_in_days" : 0,
      "max_hosts": 100,
      "max_events_to_keep_in_viewer": 5000,
      "keyword_suffix": "",
      "fields" : {
        "mapping" : {
          "timestamp" : "@timestamp",
          "hostname" : "kubernetes.node.name",
          "program": "kubernetes.container.name",
          "message": "beat.name"
        },
        "message_format": "{{{message}}}"
      },
      "color_mapping" : {
      }
    }
  ]
}